<%- include("../../partials/header") %>
<%- include("../../partials/navbar") %>
<div class="container">
  <div class="row">
    <div class="col-12">
      <h5 class="text-white my-5">Categorias</h5>
      <table class="table table-dark table-bordered text-white">
        <thead>
          <tr>
            <th scope="col">Id</th>
            <th scope="col">Título</th>
            <th scope="col">Slug</th>
            <th scope="col">Ações</th>
          </tr>
        </thead>
        <tbody>
          <% categories.map(category => { %>
          <tr>
            <th scope="row">
              <%= category.id %>
            </th>
            <td>
              <%= category.title %>
            </td>
            <td>
              <%= category.slug %>
            </td>
            <td></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>
<%- include("../../partials/footer") %>